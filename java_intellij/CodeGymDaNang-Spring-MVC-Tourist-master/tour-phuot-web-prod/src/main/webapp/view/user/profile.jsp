	</div>

	<sf:form action="show" method="GET">
		<table border="1">
			<tr>
				<td>ID</td>
				<td>Title</td>
				<td>Summary</td>
				<td>Price</td>
				<td>Place</td>
				<td>Duration</td>
			</tr>
			<c:forEach items="${list}" var="profile">
				<tr>
					<td>${profile.id}</td>
					<td>${profile.title}</td>
					<td>${profile.summary}</td>
					<td>${profile.price}</td>
					<td>${profile.serviceHostEntity.name}</td>
					<td>${profile.duration}</td>
				</tr>
			</c:forEach>
		</table>
	</sf:form>
